    Function Two_Exp (Val : In Long_Float) Return Long_Float;
	-- Returns 2 ** Val

    Function Exp (Val : In Long_Float) Return Long_Float;
	-- Returns E ** Val

    Function Power (X,Y : In Long_Float) Return Long_Float;
